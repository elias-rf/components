{
  "jsonrpc":"2.0",
  "id": "1",
  "method": "pagar_list",
  "params":{
    "sum":["VlAmortizado as ttl"],
    "where":[["CodFor","1039"],["DatPag","between",["2023-01-01","2023-12-31"]]]
  }
}
