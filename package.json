  "scripts": {
    "build:client": "vite build --config ./vite.config.ts",
    "build:server": "tsc && tsc-alias",
    "build": "npm-run-all build:client build:server",
    "bundle": "npx vite-bundle-visualizer",
    "dev:client": "cross-env NODE_ENV=development vite client --config ./vite.config.ts",
    "dev:server": "cross-env NODE_ENV=development tsx watch ./server/index.ts",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "test:client": "vitest --dir ./client",
    "test:coverage": "vitest --dir ./ --ui --coverage",
    "test:integration": "run-s test:initdb test:test",
    "test:model": "vitest --dir ./model",
    "test:server": "vitest --dir ./server",
    "test:test": "vitest --dir ./test",
    "test:utils": "vitest --dir ./utils",
    "test": "vitest --dir ./ --ui",
    "test:e2e": "playwright test --ui",
    "update": "npm-check-updates --deep --interactive",
    "track": "skott --displayMode=webapp --trackThirdPartyDependencies",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cosmos": "cosmos --lazy --port 5001",
