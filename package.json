{
  "name": "@er/intranet",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "build:client": "vite build --config ./vite.config.ts",
    "dev": "concurrently \"npm:dev:*\" ",
    "dev:client": "NODE_ENV=development vite client --config ./vite.config.ts",
    "dev:client:test": "NODE_ENV=test vite client --config ./vite.config.ts",
    "dev:server:local": "NODE_ENV=development nodemon | pino-pretty",
    "dev:server:test": "wireit",
    "dev:server:test:old": "NODE_ENV=test nodemon | pino-pretty",
    "doc": "jsdoc -c jsdoc.json",
    "start": "npm run start -w @er/server",
    "test:initdb": "NODE_ENV=test tsx ./test/aux/init-db.ts",
    "test": "vitest --dir ./",
    "test:client": "vitest --dir ./client",
    "test:server": "vitest --dir ./server",
    "test:model": "vitest --dir ./model",
    "test:utils": "vitest --dir ./utils",
    "test:test": "vitest --dir ./test",
    "test:integration": "run-s test:initdb test:test",
    "teste:e2e": "playwright test",
    "update": "npx npm-check-updates --deep --interactive",
    "storybook": "NODE_ENV=test storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "wireit": {
    "dev:server:test": {
      "command": "tsx watch ./server/index.ts | pino-pretty",
      "env": {
        "NODE_ENV": "test"
      },
      "service": true
    }
  },
