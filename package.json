{
  "name": "@er/intranet",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "build": "wireit",
    "build:client": "wireit",
    "build:server": "wireit",
    "dev:client": "NODE_ENV=development vite client --config ./vite.config.ts",
    "dev:client:test": "NODE_ENV=development vite client --config ./vite.config.ts",
    "dev:server:local": "NODE_ENV=development nodemon | pino-pretty",
    "dev:server:test": "NODE_ENV=development tsx watch ./server/index.ts",
    "start": "node dist/index.js",
    "test:initdb": "NODE_ENV=test tsx ./test/aux/init-db.ts",
    "test": "vitest --dir ./ --ui",
    "test:client": "vitest --dir ./client",
    "test:server": "vitest --dir ./server",
    "test:model": "vitest --dir ./model",
    "test:utils": "vitest --dir ./utils",
    "test:test": "vitest --dir ./test",
    "test:integration": "run-s test:initdb test:test",
    "update": "npm-check-updates --deep --interactive",
    "storybook": "NODE_ENV=development storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "build:client",
        "build:server"
      ]
    },
    "build:client": {
      "command": "vite build --config ./vite.config.ts"
    },
    "build:server": {
      "command": "tsup-node server/index.ts"
    }
  },
