  "name": "@er/intranet",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "build": "npm run build --workspaces",
    "dev": "concurrently \"npm:dev:*\" ",
    "dev:client": "vite client --config ./vite.config.ts",
    "dev:server": "NODE_ENV=development nodemon | pino-pretty",
    "dev:server:test": "NODE_ENV=test nodemon | pino-pretty",
    "doc": "jsdoc -c jsdoc.json",
    "start": "npm run start -w @er/server",
    "test:initdb": "NODE_ENV=test node -r esbuild-runner/register ./test/aux/init-db.ts",
    "test:client": "vitest --dir ./client",
    "test:server": "vitest --dir ./server",
    "test:utils": "vitest --dir ./utils",
    "test:test": "vitest --dir ./test",
    "test:integration": "run-s test:initdb test:test",
    "teste:e2e": "playwright test",
    "update": "npx npm-check-updates --deep --interactive",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
