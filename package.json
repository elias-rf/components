    "knex": "^3.1.0",
    "lru-cache": "^10.1.0",
    "pino": "^8.16.2",
    "pino-http": "^8.5.1",
    "pino-pretty": "^10.2.3",
    "tedious": "^16.6.1",
    "ts-deepmerge": "^6.2.0",
    "valibot": "^0.23.0"
